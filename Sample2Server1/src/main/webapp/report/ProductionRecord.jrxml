	<subDataset name="PRSize" uuid="d0604fa3-f9b4-4eff-aeea-97976abdb609">
		<parameter name="PRId" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select 
    pr. id,
    'Colour' colour,
    pr.s1,
     pr.s2,
     pr.s3,
     pr.s4,
     pr.s5,
     pr.s6,
     pr.s7,
     pr.s8,
     pr.s9,
     pr.s10,
     pr.s11,
     pr.s12,
     pr.s13,
     pr.s14,
     pr.s15 ,
     'Total PCS' totalpcs
FROM
     productionrecord pr
     where
pr.id=$P{PRId}
     union all
     
 select 
 prz.hdrid,
 prz.warna,
to_char(prz.q1),
to_char(prz.q2),
to_char(prz.q3),
to_char(prz.q4),
to_char(prz.q5),
to_char(prz.q6),
to_char(prz.q7),
to_char(prz.q8),
to_char(prz.q9),
to_char(prz.q10),
to_char(prz.q11),
to_char(prz.q12),
to_char(prz.q13),
to_char(prz.q14),
to_char(prz.q15),
to_char(prz.q1+prz.q2+prz.q3+prz.q4+prz.q5+prz.q6+prz.q7+prz.q8+prz.q9+prz.q10+prz.q11+prz.q12+prz.q13+prz.q14+prz.q15)
 from
 productionrecordsize prz
where
prz.hdrid=$P{PRId}
union all
 select 
 prz.hdrid,
 'Total',
to_char(sum(prz.q1)),
to_char(sum(prz.q2)),
to_char(sum(prz.q3)),
to_char(sum(prz.q4)),
to_char(sum(prz.q5)),
to_char(sum(prz.q6)),
to_char(sum(prz.q7)),
to_char(sum(prz.q8)),
to_char(sum(prz.q9)),
to_char(sum(prz.q10)),
to_char(sum(prz.q11)),
to_char(sum(prz.q12)),
to_char(sum(prz.q13)),
to_char(sum(prz.q14)),
to_char(sum(prz.q15)),
to_char(sum(prz.q1+prz.q2+prz.q3+prz.q4+prz.q5+prz.q6+prz.q7+prz.q8+prz.q9+prz.q10+prz.q11+prz.q12+prz.q13+prz.q14+prz.q15))
 from
 productionrecordsize prz
where
prz.hdrid=$P{PRId}
group by prz.hdrid]]>
		</queryString>
		<field name="ID" class="java.math.BigDecimal"/>
