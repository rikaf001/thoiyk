<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>net.sf</groupId>
  <artifactId>Sample2Client1</artifactId>
  <version>3.1</version>
  <packaging>jar</packaging>

  <name>Sample2Client1</name>

  
  <repositories>

        <repository>
          <id>maven2</id>
          <name>2Maven</name>
          <url>http://repo2.maven.org/maven2/</url>
          <layout>default</layout>
        </repository>

        <repository>
            <id>java.net-m2</id>
            <name>java.net - Maven 2</name>
            <url>https://repository.jboss.org/nexus/content/repositories/java.net-m2</url>
        </repository>     
    
  </repositories>
  
  <url>http://maven.apache.org</url>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.3.2</version>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
           
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>

  <dependencies>
          
    <dependency>
      <groupId>${project.groupId}</groupId>
      <artifactId>KFramework3Base</artifactId>
      <version>${project.version}</version>
    </dependency>
    
    <dependency>
      <groupId>${project.groupId}</groupId>
      <artifactId>KFramework3Client</artifactId>
      <version>${project.version}</version>
    </dependency>
    
    <dependency>
      <groupId>${project.groupId}</groupId>
      <artifactId>Sample2ProblemDomainComponent</artifactId>
      <version>${project.version}</version>
    </dependency>       
    
  </dependencies>
</project>
