package KFramework30.Widgets.DataBrowser;



//Variable type for filter values of SQL query
public class filterClass extends java.lang.Object {
        
    String name;
    String value;
    int    type;

    public filterClass(String nameParam,String valueParam,int typeParam){
        name = nameParam;
        value = valueParam;
        type = typeParam;
    }
}